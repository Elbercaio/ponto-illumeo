{
  "name": "ponto-illumeo",
  "version": "1.0.0",
  "description": "Sistema de ponto para Illumeo",
  "main": "index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc -p .",
    "test": "jest --coverage",
    "test:dev": "jest --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elbercaio/ponto-illumeo.git"
  },
  "keywords": [
    "react",
    "nodejs",
    "typescript"
  ],
  "author": "Elber Caio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Elbercaio/ponto-illumeo/issues"
  },
  "homepage": "https://github.com/Elbercaio/ponto-illumeo#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.31.0",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/validator": "^13.7.15",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "typescript": "^5.0.4"
  }
}
